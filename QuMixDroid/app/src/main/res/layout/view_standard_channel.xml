<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <Button
        android:id="@+id/btnChannel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal" />
    
    <org.wieggers.qu_apps.controls.bound.BoundMuteToggleButton
        android:id="@+id/boundMuteToggleButton1"
        style="@style/MuteButton" />

    <org.wieggers.qu_apps.controls.bound.BoundMixToggleButton
        android:id="@+id/boundMixToggleButton1"
        style="@style/AssignButton" />

    <org.wieggers.qu_apps.controls.bound.BoundMixToggleButton
        android:id="@+id/boundMixToggleButton2"
        style="@style/PreButton" />

    <org.wieggers.qu_apps.controls.bound.BoundMixRotaryKnob
        android:id="@+id/boundMixRotaryKnob1"
        style="@style/PanButton" />

    <org.wieggers.qu_apps.controls.bound.BoundMixFader
        android:id="@+id/boundMixFader1"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        style="@style/Fader" />
    
</merge>